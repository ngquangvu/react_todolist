{
  "name": "react-admin-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "lint": "eslint src --config .eslintrc.js --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --config .eslintrc.js --ext .js,.jsx,.ts,.tsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:snap": "vitest -u",
    "lint-staged": "yarn prettier && yarn lint && tsc --noEmit",
    "format": "yarn prettier && yarn lint:fix"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.10",
    "axios": "^0.27.2",
    "chroma-js": "^2.4.2",
    "moment": "^2.29.4",
    "react": "^18.0.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.34.2",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@tsconfig/vite-react": "^1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "happy-dom": "^4.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.9"
  }
}
